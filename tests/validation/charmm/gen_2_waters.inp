close unit 1

open read unit 2 card name "/home/noel/Projects/Protein_design/Parameters/par_all27_prot_na.prm"
read para card unit 2
close unit 2

! Read in structure files
! =======================

open read unit 1 card name "/home/noel/Projects/Protein_design/Insulin/Struct_Prep/box_setup/2hiu_box_1.psf"
read psf unit 1 card
close unit 1

open read unit 1 card name "/home/noel/Projects/Protein_design/Insulin/Struct_Prep/box_setup/2hiu_box_1.crd"
read coor unit 1 card
close unit 1

bomb -1 
!open write unit 69 form name GBSW_AB.dat

! Inter Chain Energy (A,B)
! ==============================

stream "/home/noel/Projects/Protein_design/Parameters/radii_prot_na_dfg.str"

! For two atoms only
!delete atom sele .not. ((resid 2701 .and. type OH2 .and. segid WAT) .or. (resid 2172 .and. type OH2 .and. segid WAT)) end
! For Two molecules of water
delete atom sele .not. ((resid 2701 .and.  segid WAT) .or. (resid 2172 .and. segid WAT)) end

open write unit 1 card name two_wat.crd
write coor card unit 1

write psf card name two_wat.psf

write psf card xplo name two_wat_xplo.psf

nbond -
inbfrq 1 ctonnb 960.0 ctofnb 980.0 cutnb 900.0 wmin 1.5  -
elec atom switch cdiel eps 1.0 vdw vatom vswitch -
e14fac 1.0 nbxmod 5

energy

coor force sele all end
print coor sele all end

stop

write title unit 69
*elecB, elecU, elecD, gbenB, gbenU, gbenD, vdwaB, vdwaU, vdwaD, tota
*@eleB, @eleU, @eleD, @gbeB, @gbeU, @gbeD, @vdwB, @vdwU, @vdwD, @tot 
*

close unit 69
